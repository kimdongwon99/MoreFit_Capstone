<!--글쓰는 곳 -->
<p hidden>로그인된 아이디 : <%=@email%></p>

<div class="container" >
    <h1>게시판</h1>
    <form action="/data" method="POST" enctype="multipart/form-data">
        <div class="form-group">
            <label >제목</label>
            <input type="text" name="title" class="form-control" id="title" placeholder="제품 이름을 입력하세요">
        </div>
        <div class="form-group">
            <label >가격</label>
            <input type="text" name="values" class="form-control" id="values" placeholder="제품 가격을 입력하세요">
        </div>
        <input type="hidden" name="name" class="form-control" id="name" value="<%=@email%>">
        
        <div class="form-group">
          <label for="comment">내용</label>
          <textarea class="form-control" name="content" rows="5" id="comment"></textarea>
      </div>
      
      <input type="file" name="pic" accept="image/*">
      
      <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>